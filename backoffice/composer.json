{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "w8tcha/ckeditor-wordcount-plugin",
                "version": "v1.17.6",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/w8tcha/CKEditor-WordCount-Plugin/archive/v1.17.6.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/notification",
                "version": "4.14.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/notification/releases/notification_4.14.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "drupal/base_field_display_override",
                "version": "1.0-alpha0",
                "type": "drupal-module",
                "source": {
                    "url": "https://git.drupalcode.org/sandbox/Dakwamine-3123757.git",
                    "type": "git",
                    "reference": "8.x-1.0-alpha1"
                }
            }
        }
    ],
    "require": {
        "ext-soap": "*",
        "aws/aws-sdk-php": "^3.139",
        "ckeditor-plugin/notification": "^4.14",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "dropteam/makefile": "v1.1",
        "drupal/admin_toolbar": "^2.2",
        "drupal/ala": "^1.5",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/base_field_display_override": "^1.0@alpha",
        "drupal/bulk_update_fields": "^2.0@alpha",
        "drupal/ckwordcount": "^1.0",
        "drupal/config_filter": "^2.0",
        "drupal/config_split": "^1.4",
        "drupal/consumer_image_styles": "^3.0",
        "drupal/consumers": "^1.10",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/decoupled_router": "^1.11",
        "drupal/devel": "^2.1",
        "drupal/editor_advanced_link": "^1.8",
        "drupal/entity_share": "^2.0@alpha",
        "drupal/environment_indicator": "^3.7",
        "drupal/field_group": "^3.0",
        "drupal/graphql": "^3.0",
        "drupal/health_check_url": "^2.1",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jsonapi_boost": "^1.0",
        "drupal/jsonapi_extras": "^3.14",
        "drupal/jsonrpc": "^1.0@beta",
        "drupal/link_field_autocomplete_filter": "^1.11",
        "drupal/link_target": "^1.2",
        "drupal/linkit": "^5.0",
        "drupal/mail_login": "^1.1",
        "drupal/maxlength": "^1.0@beta",
        "drupal/media_library_extend": "1.x-dev",
        "drupal/media_library_extend_youtube": "1.x-dev",
        "drupal/menu_item_extras": "^2.10",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^1.13",
        "drupal/openapi": "^2.0@RC",
        "drupal/openapi_jsonapi": "^2.0@RC",
        "drupal/openapi_ui": "^1.0@RC",
        "drupal/openapi_ui_redoc": "^1.0@RC",
        "drupal/paragraphs": "^1.11",
        "drupal/paragraphs_browser": "^1.0@beta",
        "drupal/password_policy": "^3.0",
        "drupal/path_redirect_import": "^1.0@beta",
        "drupal/pathauto": "^1.8",
        "drupal/pathologic": "^1.0",
        "drupal/redirect": "^1.5",
        "drupal/redirect_after_login": "^2.5",
        "drupal/remove_entity_untranslatable_field_validation": "^1.0",
        "drupal/reroute_email": "^1.1",
        "drupal/s3fs": "^3.0@alpha",
        "drupal/schemata": "^1.0@beta",
        "drupal/simple_menu_permissions": "^1.2",
        "drupal/simple_oauth": "^4.5",
        "drupal/subrequests": "^2.2",
        "drupal/svg_image": "^1.14",
        "drupal/term_reference_tree": "^1.0",
        "drupal/token": "^1.6",
        "drupal/toolbar_menu": "^2.1",
        "drupal/translatable_menu_link_uri": "^1.2",
        "drupal/transliterate_filenames": "^1.4",
        "drupal/warmer": "^1.1",
        "drupal/workbench_email": "^1.4",
        "drush/drush": "^10.2",
        "oomphinc/composer-installers-extender": "^1.1",
        "w8tcha/ckeditor-wordcount-plugin": "^1.17"
    },
    "require-dev": {
        "mglaman/phpstan-drupal": "^0.12.2",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.18",
        "phpstan/phpstan-deprecation-rules": "^0.12.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-types": [
            "dropteam-makefile"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "{$name}": [
                "type:dropteam-makefile"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ]
        },
        "patches": {
            "drupal/core": {
                "Taxonomy Index for unpublished entities": "https://www.drupal.org/files/issues/2020-04-16/962664-drupal-add-unpublished-nodes-to-taxonomy-index-149.patch",
                "AccessAwareRouter does not respect HTTP method": "https://www.drupal.org/files/issues/2018-12-12/2706241-34.patch"
            },
            "drupal/reroute_email": {
                "Add method to log messages to Drupal log for debugging purposes": "https://www.drupal.org/files/issues/2019-02-05/reroute_email-log-messages-sent-3030877-01.patch"
            },
            "drupal/translatable_menu_link_uri": {
                "WSOD error during enabling the module": "https://www.drupal.org/files/issues/2020-02-17/unable_to_install_module-3114114-2.patch"
            },
            "drupal/simple_menu_permissions": {
                "Prefix lang on request uri": "web/modules/patchs/simple_menu_permissions-prefix_lang.patch"
            },
            "drupal/linkit": {
                "Show the alias after the autocomplete selection": "https://www.drupal.org/files/issues/2020-03-11/2877535-12.patch"
            },
            "drupal/link_target": {
                "Make target options translatable": "https://www.drupal.org/files/issues/2019-10-09/link_target-3086791-2.patch"
            }
        }
    }
}
